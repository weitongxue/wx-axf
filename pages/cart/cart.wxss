.main{
  bottom:0;
}
.notic-wrap{
  text-align: center;
  margin-top: 20%;
  padding: 2rem 0;
  position: absolute;
  top: 0;
  bottom:0;
  left: 0;
  right: 0;
}
.notic-img{
    width:160rpx;
    height: 160rpx;
    background: url("data:image/png;base64,") 0 0 no-repeat;
    background-size: 160rpx;
    margin-left: 38%;
  }
  .notic-wrap text{
    width:100%;
    display: inline-block;
    margin:40rpx 0;
    font-size: 28rpx;
      color:#999; 
  }
  .notic-wrap text:nth-child(3){
      width: 220rpx;
      height: 60rpx;
      border:1px solid #ccc;
      border-radius: 10rpx;
      line-height: 60rpx;
  }
  .user-news {
  font-size: 30rpx;
  padding: 40rpx 0;
  width: 100%;
  background-image: url('data:image/png;base64,'),url('data:image/png;base64,');
  background-position: left top,left bottom;
  background-size: auto 6rpx;
  background-color: #ffffff;
  background-repeat: repeat-x;
  margin-bottom: 20rpx;
  position: relative;
  }
   .user-news .tr {
     margin-bottom: 20rpx;
   }
   .user-news .th{
    padding: 14rpx 20rpx;
    text-align: left;
  }
.user-news .td{
    padding: 14rpx 0;
  }
 .user-news .amend {
   font-size: 35rpx;
   position: absolute;
   right: 0;
   top:40%;
    width: 18%;
    background: url('data:image/png;base64,') right center no-repeat;
    background-size: 50rpx;
  }
  .cart-group-wrap{
  background-color: #fff;
  position: relative;
  }
  .cart-market-wrap{
    font-size: 28rpx;
     height: 70rpx; 
    line-height: 60rpx;
    padding-left: 28rpx;
     color: #999999; 
  }
  .cart-market-wrap::before{
    content: '';
    display: inline-block;
    width: 8rpx;
    height: 24rpx;
    background-color: #ffd600; 
    margin-right: 6rpx;
    border-radius: 4rpx;
    vertical-align: -4rpx;
  }  
 .cart-group-wrap  .qisong{
    text-align: center;
     font-size: 28rpx;
      color:#999;
      padding-left: 26rpx;
      height: 44rpx;
      line-height: 40rpx;
      margin :12rpx 0;
  }
  .cart-addBtn{
    position: absolute;
     right: 28rpx; 
    top: 10rpx;
    font-size: 28rpx;
    width: 140rpx;
     height: 49rpx; 
    line-height: 17rpx;
    border-radius: 24rpx;
    border:1px solid #f40;
    color: #f40;
    text-align: center;
  }
  .section{
    position: relative;
    border: 1px solid white;
    box-sizing: border-box;
    border-top-color: lightgray;
    border-bottom-color: lightgray;
    height: 100rpx;
    line-height: 96rpx;
    font-size: 28rpx;
    padding: 0  26rpx; 
    background: url('data:image/png;base64,') right center no-repeat;
    background-size: 40rpx;
    overflow: hidden;
  }
  .yuding{
  float: right;
  padding-right: 28rpx;
}
.picker-time{
  color:red;
  padding: 0 40rpx;
}
.cart-receive-remark{
    height:100rpx;
    line-height:100rpx;
    font-size: 28rpx;
    padding-left: 26rpx;
    padding-right: 36rpx;
}
.remark-input{
      top:22rpx;
      position: relative;
      height: 64rpx;
      line-height: 64rpx;
      outline: none;
      font-size: 28rpx;
      padding-left: 20rpx;
       border: 0;
      border-radius: 12rpx;
      width: 75%;
      display: inline-block;
}
.group-item-wrap{
  width:100%;
  background-color: #fff;
  position: relative;
  overflow: hidden;
}
.group-item{
    width: 100%;
    overflow: hidden;
    border-top: 1px solid lightgray;
    border-bottom: 1px solid lightgray;
 }
 .group-item-checkbox{
    float:left;
    margin-top: 98rpx;
    background: url("data:image/png;base64,") center center no-repeat;
      display: block;
      -webkit-background-size: 36rpx 36rpx;
      background-size: 36rpx 36rpx;
      width: 100rpx;
}
.group-item-checkbox.active{
      background-image: url("data:image/png;base64,");
    }
 .group-item-img{
     float:left;
      width: 200rpx;
      height: 200rpx; 
      padding: 20rpx 0; 
 }
  .group-item-img image{
    width: 100%;
     height: 100%; 
  }
    .group-item-detail{
      float:left;
      font-size: 26rpx;
      margin-top: 20rpx;
      height: 100rpx;
    }
    .group-item-name{
      height: 80rpx;
      line-height: 80rpx;
      padding-left:20rpx;
    }
    .product-specifics-wrap{
      padding-left:20rpx;
      margin-top: 55rpx;
    }
.btn-wrap{
  position: absolute;
  right: 0;
  bottom:20rpx;
  width: 180rpx;
  height:100rpx;
  line-height: 120rpx;
}
.btn-wrap text:nth-child(2n+1){
    display: inline-block;
    width: 50rpx;
    height: 50rpx;
    line-height: 44rpx;
    text-align: center;
    border: 1px solid #f40;
    color: #f40;
    border-radius: 50%;
    font-size: 48rpx;
  }
.btn-wrap text:nth-child(2){
    display: inline-block;
    width: 40rpx;
    height: 60rpx;
    line-height: 50rpx;
    text-align: center;
  }

  .footer{
  position: relative;
  width:100%;
  height:100rpx;
  background-color: white;
  font-size: 28rpx;
  }
  .footer text{
    height: 100%;
    line-height: 100rpx;
  }
  .checked{
    padding-left: 50rpx;
    display: inline-block;
    width: 120rpx;
    background:url('data:image/png;base64,') 4rpx center no-repeat;
    background-size: 36rpx 36rpx;
    margin-left: 28rpx;
  }
  .checked.active{
     background:url('data:image/png;base64,') 4rpx center no-repeat;
    background-size: 36rpx 36rpx;
  }
  .total{
    color:#f40;
  }
  .sec{
    text-align: center;
    width: 200rpx;
    position: absolute;
    top: 0;
    right: 0;
    background-color: #808080;
  }
  .sec.active{
     background-color: #ffd600;
  }
